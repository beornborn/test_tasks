<%=render "post_socialconnect"%>
<div id="content">  
    <div id="content_left">
      <%=render :partial=>"entitys/post_entity"%>
      <%=render :partial=> "entitys/entitys"%>
    </div>

    <div id="content_right">
       <%= render :partial => "entitys/social_find_us" %>
       <%= render :partial => "entitys/breaking_entitys" unless Breakingentity.where("order_number > '0'").count==0 %>
       <%= render :partial => "entitys/trending_entitys" %>
       <%= render :partial => "entitys/poll_questions" %>
       <%= render :partial => "shared/footer" %>
    </div>
    <div class="clr"></div>
</div>

<script type="text/javascript">
  <% if (flash[:alert] == I18n.t("devise.failure.already_authenticated")) %>
    $(document).ready(function(){
      window.parent.location = "/"
    });
  <% end %>
</script>