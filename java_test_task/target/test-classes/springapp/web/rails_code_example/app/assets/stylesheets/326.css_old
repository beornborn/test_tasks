.pagination{
    float: left;
    margin: 0;
    padding: 10px;
    border: 0;
    outline: 0;
    overflow: hidden;
    display: block;
    margin-right: 75px;

}

.pagination span{
    list-style-type: none;
    margin-left: 0px;
    white-space: nowrap;
    display: inline;
    float: left;
    padding-left: 4px;
    padding-right: 4px;
    color: black;
}
.pagination span.current{
    display: -moz-inline-box;
    -moz-box-orient: vertical;
    display: inline-block;
    vertical-align: middle;
    padding: 6px 8px;
    line-height: 1;
    font-size: 15px;
    font-weight: bold;
    color: white;
    /*    border: solid 2px red;*/
    background-color: #ff2010;

    margin-right: 3px;
    text-shadow: 0px 1px 1px rgba(0, 0, 0, 0.5);
    -webkit-box-shadow: 0px 1px 2px rgba(0,0,0,0.1);
    -moz-box-shadow: 0px 1px 2px rgba(0,0,0,0.1);
    box-shadow: 0px 1px 2px rgba(0,0,0,0.1);
}
.pagination span a{
    display: inline-block;
    vertical-align: middle;
    padding: 5px 8px;
    line-height: 1;
    font-size: 15px;
    font-weight: bold;

    border: solid 1px #BBB;
    background-color: white;
    margin-right: 3px;
    color: #333;
}

