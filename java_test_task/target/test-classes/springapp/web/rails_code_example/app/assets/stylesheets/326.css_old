#fancybox-close {
    background: url("cross_image_n.png") no-repeat transparent;
    cursor: pointer;
    display: none;
    height: 50px;
    position: absolute;
    right: -40px;
    top: -35px;
    width: 50px;
    z-index: 1103;
}
#fancybox-close:hover {
    background-position: 0 -50px;
}
#fancybox-bg-n {
    background: url("top.png") repeat-x transparent;
    left: 0;
    top: -20px;
    width: 100%;
}
#fancybox-bg-nw {
    background-image: url("tl.png");
    left: -20px;
    top: -20px;
    background-position: 0 0;
}
#fancybox-bg-w {
    background: url("left.png") repeat-y transparent;
    left: -20px;
    top: 0;
    height: 100%;
}
#fancybox-bg-sw {
    background: url("bl.png") no-repeat scroll 0 0 transparent;
    width: 125px;
    height: 50px;
    bottom: -50px;
}
#fancybox-bg-s {
    background: url("bottom.png") repeat-x scroll 0 0 transparent;
    left: 0;
    width: 100%;
    bottom: -50px;
    height: 50px;
    margin-left: 10px;
}
#fancybox-bg-se {
    background: url("br.png") no-repeat scroll 0 0 transparent;
    height: 50px;
    bottom: -50px;
    right: -35px;
    width: 35px;
    z-index: 1002;
}
#fancybox-bg-e {
    background: url("right.png") repeat-y transparent;
    right: -35px;
    top: 0;
    height: 100%;
    width: 35px;
}

