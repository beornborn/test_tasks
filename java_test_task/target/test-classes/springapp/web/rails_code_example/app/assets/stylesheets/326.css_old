/*
Main
**********************/
#makethemround2 {
    background-color: #231F20;
    *zoom: 1;
    filter: progid:DXImageTransform.Microsoft.gradient(gradientType=0, startColorstr='#FF000000', endColorstr='#FF414042');
    background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #000000), color-stop(100%, #414042));
    background-image: -webkit-linear-gradient(top, #000000 0%, #414042 100%);
    background-image: -moz-linear-gradient(top, #000000 0%, #414042 100%);
    background-image: -o-linear-gradient(top, #000000 0%, #414042 100%);
    background-image: -ms-linear-gradient(top, #000000 0%, #414042 100%);
    background-image: linear-gradient(top, #000000 0%, #414042 100%);
    float: right;
    margin: auto;
    width: 255px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -o-border-radius: 5px;
    -ms-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
    -moz-box-shadow: -1px -2px 5px 0 #888888;
    -webkit-box-shadow: -1px -2px 5px 0 #888888;
    -o-box-shadow: -1px -2px 5px 0 #888888;
    box-shadow: -1px -2px 5px 0 #888888;
}
#makethemround2 #tagCloud {
    font: 12px Arial, Helvetica, sans-serif;
    padding: 20px;
    position: relative;
    min-height: 400px;
}
#makethemround2 #tagCloud a {
    color: #fff;
    text-decoration: none;
}
#makethemround2 #tagCloud h2 {
    color: #fff;
    margin-bottom: 10px;
}
#makethemround2 #tagCloud ul {
    margin: 0;
    padding: 0;
    width: auto;
}
#makethemround2 #tagCloud li {
    background: #ff2010;
    display: inline-block;
    font-family: AlphabetSoup, sans-serif;
    letter-spacing: 1px;
    position: relative;
    text-align: center;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.2), -1px 0 rgba(0, 0, 0, 0.2);
    padding: 3px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -o-border-radius: 5px;
    -ms-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;
    -moz-transform: skew(0, -10deg);
    -webkit-transform: skew(0, -10deg);
    -o-transform: skew(0, -10deg);
    -ms-transform: skew(0, -10deg);
    transform: skew(0, -10deg);
}
#makethemround2 #tagCloud li a {
    display: inline-block;
    background: #ffc500;
    padding: 2px 6px;
    text-transform: uppercase;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    -o-border-radius: 3px;
    -ms-border-radius: 3px;
    -khtml-border-radius: 3px;
    border-radius: 3px;
}

